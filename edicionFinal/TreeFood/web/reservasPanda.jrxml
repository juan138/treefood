	<queryString>
		<![CDATA[SELECT
     reservas.`id_reserva` AS reservas_id_reserva,
     reservas.`fecha_reserva` AS reservas_fecha_reserva,
     reservas.`numero_contacto` AS reservas_numero_contacto,
     reservas.`email` AS reservas_email,
     reservas.`estado` AS reservas_estado,
     reservas.`area_restaurante` AS reservas_area_restaurante,
     clientes.`Email` AS clientes_Email,
     clientes.`telefono` AS clientes_telefono,
     sucursales.`sucursal` AS sucursales_sucursal,
     personas.`nombres` AS personas_nombres,
     personas.`apellidos` AS personas_apellidos,
     sucursales_restaurantes.`id_restaurante` AS sucursales_restaurantes_id_restaurante,
     sucursales_restaurantes.`id_sucursal` AS sucursales_restaurantes_id_sucursal
FROM
     `clientes` clientes INNER JOIN `reservas` reservas ON clientes.`id_cliente` = reservas.`id_cliente`
     INNER JOIN `sucursales` sucursales ON reservas.`sucursal` = sucursales.`id_sucursal`
     INNER JOIN `sucursales_restaurantes` sucursales_restaurantes ON sucursales.`id_sucursal` = sucursales_restaurantes.`id_sucursal`
     INNER JOIN `personas` personas ON clientes.`id_persona` = personas.`id_persona`
WHERE
     reservas.`estado` = "activa" and sucursales.`id_sucursal` = $P{sucursal}]]>
	</queryString>
